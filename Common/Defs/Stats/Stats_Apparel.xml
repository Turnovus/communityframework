<?xml version="1.0" encoding="UTF-8" ?>
<Defs>
    <StatDef>
        <defName>CF_CaravanCapacityOffset</defName>
        <label>caravan capacity offset</label>
        <description>An offset to how much maximum mass the wearer contributes to its caravan.</description>
        <category>Apparel</category>
        <defaultBaseValue>0</defaultBaseValue>
        <toStringStyle>Integer</toStringStyle>
        <showOnAnimals>false</showOnAnimals>
        <showOnPawns>false</showOnPawns>
        <showIfUndefined>false</showIfUndefined>
        <displayPriorityInCategory>3504</displayPriorityInCategory>
        <parts>
            <li Class="StatPart_Quality">
                <!-- Same as SmokepopBeltRadius -->
                <factorAwful>0.84</factorAwful>
                <factorPoor>0.92</factorPoor>
                <factorNormal>1</factorNormal>
                <factorGood>1.08</factorGood>
                <factorExcellent>1.16</factorExcellent>
                <factorMasterwork>1.3</factorMasterwork>
                <factorLegendary>1.5</factorLegendary>
                <applyToNegativeValues>true</applyToNegativeValues>
            </li>
        </parts>
    </StatDef>
</Defs>